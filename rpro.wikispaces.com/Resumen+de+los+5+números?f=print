</style><pre class="rsplus"><span class="co1"># Resumen de los 5 números de Tukey aplicado a un vector</span>
&nbsp;
<span class="kw2">set.<span class="me1">seed</span></span><span class="br0">&#40;</span><span class="nu0">10</span><span class="br0">&#41;</span>
x <span class="sy0">&lt;-</span> <span class="kw7">rnorm</span><span class="br0">&#40;</span><span class="nu0">50</span>,<span class="nu0">10</span>,<span class="nu0">2</span><span class="br0">&#41;</span> <span class="co1"># 50 números aleatorios de media 10 y desviación típica 2</span>
&nbsp;
cinco <span class="sy0">&lt;-</span> <span class="kw7">fivenum</span><span class="br0">&#40;</span>x<span class="br0">&#41;</span>
seis <span class="sy0">&lt;-</span> <span class="kw2">summary</span><span class="br0">&#40;</span>x<span class="br0">&#41;</span>
&nbsp;
<span class="kw2">cat</span><span class="br0">&#40;</span><span class="st0">&quot;<span class="es0">\n</span>Resumen de los 5 números:<span class="es0">\n</span>&quot;</span><span class="br0">&#41;</span>
<span class="kw2">print</span><span class="br0">&#40;</span>cinco<span class="br0">&#41;</span>
&nbsp;
&nbsp;
<span class="kw4">boxplot</span><span class="br0">&#40;</span>x,main<span class="sy0">=</span><span class="st0">&quot;Resumen de los 5 números&quot;</span><span class="br0">&#41;</span>
&nbsp;
&nbsp;
<span class="kw2">cat</span><span class="br0">&#40;</span><span class="st0">&quot;<span class="es0">\n</span>Resumen de los 5 número más la medias:<span class="es0">\n</span>&quot;</span><span class="br0">&#41;</span>
<span class="kw2">print</span><span class="br0">&#40;</span>seis<span class="br0">&#41;</span>
<span class="kw4">boxplot</span><span class="br0">&#40;</span>x,main<span class="sy0">=</span><span class="st0">&quot;Resumen de los 5 números + media&quot;</span><span class="br0">&#41;</span>
<span class="kw4">points</span><span class="br0">&#40;</span><span class="kw2">mean</span><span class="br0">&#40;</span>x<span class="br0">&#41;</span>,pch<span class="sy0">=</span><span class="nu0">4</span>,<span class="kw2">col</span><span class="sy0">=</span><span class="st0">&quot;red&quot;</span><span class="br0">&#41;</span></pre>


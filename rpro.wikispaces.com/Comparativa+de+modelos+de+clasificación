  <span class="co1"># Árboles de clasificación: rpart</span>
  <span class="kw2">library</span><span class="br0">&#40;</span>rpart<span class="br0">&#41;</span>
  modelo.<span class="me1">rp</span> <span class="sy0">&lt;-</span> rpart<span class="br0">&#40;</span>f, <span class="kw8">data</span><span class="sy0">=</span>datos.<span class="me1">train</span><span class="br0">&#41;</span>

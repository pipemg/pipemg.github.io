  mc <span class="sy0">&lt;-</span> mc<span class="br0">&#91;</span><span class="kw2">order</span><span class="br0">&#40;</span><span class="kw2">rownames</span><span class="br0">&#40;</span>mc<span class="br0">&#41;</span><span class="br0">&#41;</span>,<span class="kw2">order</span><span class="br0">&#40;</span><span class="kw2">colnames</span><span class="br0">&#40;</span>mc<span class="br0">&#41;</span><span class="br0">&#41;</span><span class="br0">&#93;</span>
  <span class="kw2">cat</span><span class="br0">&#40;</span><span class="st0">&quot;** Árboles de clasificación: rpart<span class="es0">\n</span>&quot;</span><span class="br0">&#41;</span>
  <span class="kw2">print</span><span class="br0">&#40;</span>mc<span class="br0">&#41;</span>

    mc <span class="sy0">&lt;-</span> <span class="kw2">table</span><span class="br0">&#40;</span>pred, datos.<span class="me1">test</span><span class="br0">&#91;</span>,dep<span class="br0">&#93;</span>, dnn <span class="sy0">=</span> <span class="kw2">c</span><span class="br0">&#40;</span><span class="st0">&quot;Asignado&quot;</span>,<span class="st0">&quot;Real&quot;</span><span class="br0">&#41;</span><span class="br0">&#41;</span>
